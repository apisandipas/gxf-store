<section class="users">
<table>
  <thead>
    <th>Email</th>
    <th></th>
    <th></th>
  </thead>

<% @users.each do |user| %>
  <tr>
    <td><%=  user.email %></td>
   
    <td><%= link_to 'Edit', edit_admin_user_path(user) %></td>
    <td><%= link_to 'Destroy', admin_user_path(user), method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>

</table>

<%= link_to 'New User', new_admin_user_path %>


</section>

